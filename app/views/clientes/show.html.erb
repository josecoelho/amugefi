<div class="page-header">
  <%= link_to clientes_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Voltar
  <% end %>
  <%= link_to edit_cliente_path(@cliente), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Editar
  <% end %>
  <h1>Mostrar cliente</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nome:</dt>
  <dd><%= @cliente.nome %></dd>

  <dt>Nome fantasia:</dt>
  <dd><%= @cliente.nome_fantasia %></dd>

  <dt>Cnpj:</dt>
  <dd><%= @cliente.cnpj %></dd>

  <dt>Endereco:</dt>
  <dd><%= @cliente.endereco %></dd>

  <dt>Email:</dt>
  <dd><%= @cliente.email %></dd>

</dl>
